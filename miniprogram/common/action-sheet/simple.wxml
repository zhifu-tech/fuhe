<t-action-sheet wx:if="{{ simpleActionSheet.enabled }}"
  id="t-action-sheet"
  visible="{{ simpleActionSheet.visible }}"
  usingCustomNavbar
  items="{{ simpleActionSheet.items }}"
  popupProps="{{ simpleActionSheet.popupProps }}"
  bind:cancel="_handleSimpleActionSheetClose"
  bind:close="_handleSimpleActionSheetClose"
  bind:selected="_handleSimpleActionSheetSelected" />
