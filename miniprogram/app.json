{
  "cloud": true,
  "sitemapLocation": "sitemap.json",
  "darkmode": true,
  "resizable": false,
  "frameset": false,
  "window": {
    "navigationStyle": "custom"
  },
  "lazyCodeLoading": "requiredComponents",
  "renderer": "webview",
  "rendererOptions": {
    "skyline": {
      "defaultDisplayBlock": true,
      "defaultContentBox": true,
      "disableABTest": true
    }
  },
  "componentFramework": "glass-easel",
  "pages": [
    "pages/dashboard/index",
    "pages/store/index",
    "pages/order/index"
  ],
  "subpackages": [
    {
      "root": "package-goods",
      "pages": []
    },
    {
      "root": "package-mix",
      "pages": [
        "entity/list/index"
      ]
    },
    {
      "root": "package-cso",
      "pages": [
        "category/list/index"
      ]
    },
    {
      "root": "package-cart",
      "pages": []
    },
    {
      "root": "package-dashboard",
      "pages": []
    }
  ],
  "tabBar": {
    "custom": true,
    "list": [
      {
        "pagePath": "pages/store/index"
      },
      {
        "pagePath": "pages/order/index"
      },
      {
        "pagePath": "pages/dashboard/index"
      }
    ]
  },
  "resolveAlias": {
    "@/*": "/*"
  },
  "debug": true,
  "debugOptions": {
    "enableFPSPanel": false
  }
}