{
  "cloud": true,
  "sitemapLocation": "sitemap.json",
  "darkmode": false,
  "resizable": true,
  "frameset": true,
  "useExtendedLib": {
    "weui": true
  },
  "pages": [
    "pages/store/index",
    "pages/cart/index",
    "pages/user/index",
    "pages/shelf/index"
  ],
  "subpackages": [
    {
      "root": "package_home",
      "pages": [
        "pages/mock/index"
      ]
    },
    {
      "root": "package_user",
      "pages": [
        "pages/person-info/index"
      ]
    }
  ],
  "window": {
    "backgroundTextStyle": "light",
    "navigationBarBackgroundColor": "#fff",
    "navigationBarTitleText": "福猫",
    "navigationBarTextStyle": "black"
  },
  "tabBar": {
    "custom": true,
    "color": "#7A7E83",
    "selectedColor": "#3cc51f",
    "borderStyle": "black",
    "backgroundColor": "#F7F7F7",
    "list": [
      {
        "pagePath": "pages/store/index"
      },
      {
        "pagePath": "pages/cart/index"
      },
      {
        "pagePath": "pages/user/index"
      }
    ]
